#include <stdio.h>
int main() {
    int a, b, c, sum = 0;
    printf("Enter any number to check whether it is palindrme number or not:");
    scanf("%d, &c");
    a = c;
    while(a>=1) {
        b= a%10;
        a = a/10;
        sum =sum*10+b;
        
  }
     if (sum == c) {
         printf("\nEntered number is a Panlindrome Number");
         
     }
      return 0;
}



