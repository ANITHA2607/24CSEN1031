#include <stdio.h>

int main() {
    int a, b, c, sum = 0;
    printf("Enter any number to be reversed:");
    scanf("%d",&c);
    a = c;
    printf("\n\n");
    printf("after reversing :\n");
    
    while (a>=1) {
        b = a%10;
        a = a/10;
        printf("\n%d,b");
    }
    return 0 ;
    }



OUTPUT

enter any number to be reversed:55258


after reversing:
8
5
